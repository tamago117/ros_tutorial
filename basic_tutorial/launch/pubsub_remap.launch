<launch>
  <node name="number_publisher"   pkg="rosLecture_kobe" type="number_publisher" >
    <remap from="chatter" to="number"/>
  </node>
  <node name="number_multiplier"   pkg="rosLecture_kobe" type="number_multiplier" >
